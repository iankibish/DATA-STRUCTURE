# Node class
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

# Linked List
class LinkedList:
    def __init__(self):
        self.head = None

    def insert(self, data):
        new_node = Node(data)
        new_node.next = self.head
        self.head = new_node

    def display(self):
        current = self.head
        while current:
            print(current.data)
            current = current.next

ll = LinkedList()
ll.insert(1)
ll.insert(2)
ll.display()
